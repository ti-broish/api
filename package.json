{
  "name": "@dabulgaria/ti-broish-api",
  "description": "Ti Broish API built with Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "cp -r src/seeds/parl-2021-04-04 dist/seeds/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:db": "./scripts/start-dev-db.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "typeorm migration:create",
    "migration:generate": "typeorm migration:generate",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "seed:create": "typeorm --connection seed migration:create",
    "seed:run": "typeorm --connection seed migration:run",
    "seed:revert": "typeorm --connection seed migration:revert",
    "firebase:token": "node ./scripts/get-token.js"
  },
  "dependencies": {
    "@anchan828/typeorm-decorators": "^0.4.11",
    "@casl/ability": "^5.2.0",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.5.4",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.4",
    "@nestjs/passport": "^7.1.3",
    "@nestjs/platform-express": "^7.5.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/typeorm": "^7.1.5",
    "@sindresorhus/slugify": "^1.1.0",
    "@tfarras/nestjs-firebase-admin": "^1.0.1",
    "@tfarras/nestjs-firebase-auth": "^1.0.0",
    "@types/joi": "^14.3.4",
    "base64-img": "^1.0.4",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "csv-parse": "^4.14.1",
    "dateformat": "^4.4.1",
    "firebase-admin": "^8.13.0",
    "install": "^0.13.0",
    "joi": "^17.3.0",
    "lodash.merge": "^4.6.2",
    "mime-types": "^2.1.27",
    "nestjs-i18n": "^8.0.11",
    "nestjs-minio": "^2.0.10",
    "nestjs-typeorm-paginate": "^2.3.0",
    "npm": "^6.14.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "stream-transform": "^2.0.3",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.4",
    "@types/cron": "^1.7.2",
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/mime-types": "^2.1.0",
    "@types/minio": "^7.0.6",
    "@types/node": "^14.14.9",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-idtoken-getter": "^1.0.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Da-Bulgaria/ti-broish-api.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/Da-Bulgaria/ti-broish-api/issues"
  },
  "homepage": "https://github.com/Da-Bulgaria/ti-broish-api#readme"
}
